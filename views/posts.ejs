<% include partials/header.ejs %>

<h1>Post List</h1>

<% for (let i = 0; i < posts.length; i ++) { %>
<h2>
    <%= posts[i].title %> - <%= posts[i].author %>
</h2>
<% } %>

<% posts.forEach(function(post) { %>
<h2>
    <%= post.title %> - <%= post.author %>
</h2>
<% }) %>

<% include partials/footer.ejs %>